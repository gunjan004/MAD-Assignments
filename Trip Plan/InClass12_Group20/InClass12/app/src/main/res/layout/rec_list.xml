<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/rec_tipName"
        android:layout_width="343dp"
        android:layout_height="29dp"
        android:layout_alignStart="@+id/rec_DestCity"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-127dp"
        android:layout_marginTop="22dp"
        android:text="TextView"
        android:textColor="@color/common_google_signin_btn_text_dark_focused" />

    <TextView
        android:id="@+id/rec_DestCity"
        android:layout_width="100dp"
        android:layout_height="22dp"
        android:layout_alignStart="@+id/rec_userName"
        android:layout_alignParentTop="true"
        android:layout_marginStart="127dp"
        android:layout_marginTop="53dp"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:text="TextView" />

    <TextView
        android:id="@+id/rec_date"
        android:layout_width="89dp"
        android:layout_height="22dp"
        android:layout_alignTop="@+id/rec_userName"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="14dp"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:text="TextView" />

    <TextView
        android:id="@+id/rec_userName"
        android:layout_width="110dp"
        android:layout_height="22dp"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="52dp"
        android:text="TextView" />
</RelativeLayout>